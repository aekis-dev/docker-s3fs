{
    "description": "Docker S3FS Volume Plugin",
    "documentation": "https://github.com/aek/docker-s3fs/",
    "entrypoint": [
        "/sbin/tini",
        "--",
        "/docker-s3fs"
    ],
    "env": [
        {
            "name": "DEFAULT_S3FSOPTS",
            "settable": [
                "value"
            ],
            "value": ""
        }
    ],
    "network": {
        "type": "host"
    },
    "propagatedMount": "/var/lib/docker-volumes",
    "interface": {
        "types": [
            "docker.volumedriver/1.0"
        ],
        "socket": "s3fs.sock"
    },
    "linux": {
        "capabilities": [
            "CAP_SYS_ADMIN"
        ],
        "devices": [
            {
                "path": "/dev/fuse"
            }
        ]
    }
}
